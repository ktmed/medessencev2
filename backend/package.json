{
  "name": "radiology-ai-api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for Radiology AI System - Medical-grade backend with authentication, authorization and service orchestration",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "echo 'No build step needed for JavaScript project'",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --ci --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force",
    "docker:build": "docker build -t radiology-ai-gateway .",
    "docker:run": "docker run -p 3000:3000 radiology-ai-gateway",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "api-gateway",
    "medical",
    "radiology",
    "healthcare",
    "jwt",
    "authentication",
    "microservices",
    "gdpr-compliant"
  ],
  "author": "Medical AI Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openapi-validator": "^5.1.6",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "opossum": "^8.0.0",
    "prom-client": "^15.1.0",
    "redis": "^4.6.11",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.8.0",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/med-essence/radiology-ai-system.git"
